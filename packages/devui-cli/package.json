{
  "name": "@devui/devui-cli",
  "version": "0.0.1",
  "description": "A scaffold of devui.",
  "main": "dist/node/index.js",
  "types": "dist/node/index.d.ts",
  "bin": {
    "devui-cli": "bin/devui-cli.js"
  },
  "scripts": {
    "prepare": "pnpm build",
    "dev": "rimraf dist && rollup -c -w",
    "build": "rimraf dist && rollup -c"
  },
  "files": [
    "bin",
    "dist",
    "types",
    "site"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TinsFox/devui-cli.git"
  },
  "keywords": [
    "devui",
    "devui-cli",
    "cli"
  ],
  "engines": {
    "node": ">=12"
  },
  "author": "TinsFox",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TinsFox/devui-cli/issues"
  },
  "homepage": "https://github.com/TinsFox/devui-cli#readme",
  "dependencies": {
    "resolve": "^1.22.0",
    "rollup": "^2.59.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged --concurrent false",
    "commit-msg": "pnpm exec ts-node scripts/verifyCommit.ts $1"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "13.1.3",
    "@rollup/plugin-typescript": "^8.3.1",
    "@vue/compiler-dom": "^3.2.31",
    "cac": "6.7.9",
    "fast-glob": "^3.2.11",
    "magic-string": "^0.26.1",
    "picocolors": "^1.0.0",
    "rollup-plugin-license": "^2.6.1",
    "source-map-support": "^0.5.21",
    "terser": "^5.12.1",
    "tslib": "^2.3.1",
    "typescript": "^4.6.2",
    "simple-git-hooks": "^2.7.0"
  }
}
